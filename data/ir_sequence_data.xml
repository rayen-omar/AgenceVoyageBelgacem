<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_tour_package" model="ir.sequence">
            <field name="name">Forfait Touristique Sequence</field>
            <field name="code">tour.package</field>
            <field name="prefix">TP/</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
        </record>

        <record id="seq_travel_reservation" model="ir.sequence">
            <field name="name">Reservation Sequence</field>
            <field name="code">travel.reservation</field>
            <field name="prefix">RES/</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
        </record>

        <record id="seq_travel_payment" model="ir.sequence">
            <field name="name">Payment Sequence</field>
            <field name="code">travel.payment</field>
            <field name="prefix">PAY/</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
        </record>
    </data>
</odoo>